         <script>
                function uncheckOthers(a){
                        if(a.checked == true){
                                if(a.id.indexOf("shipID") != -1){
                                var elements = document.getElementsByTagName("input");
                                        for(i=0;i<elements.length;i++)
                                                {
                                                    e = elements[i];
                                                    if(e.id.indexOf("shipID") != -1){
                                                                if(a.id != e.id){
                                                                        e.checked = false;
                                                                }
                                                    }
                                            }
                                        }
                                }else{
                                                if(a.checked == true){
                                                var elements = document.getElementsByTagName("input");
                                                        for(i=0;i<elements.length;i++)
                                                                {
                                                                    e = elements[i];
                                                                    if(e.id.indexOf("billID") != -1){
                                                                                if(a.id != e.id){
                                                                                        e.checked = false;
                                                                                }
                                                                    }
                                                            }
                                                        }
                        
                                                }
                                       }
                        
                function CloseAndRefresh(){
                        
                       
                      window.top.close();  
                                            
                  
                  }
                  
                function noenterBill(e){
                                if(window.event){                    
                                        key = window.event.keyCode;     //IE               
                                } 
                                else{                    
                                        key = e.which;     //firefox               
                                }               
                                if(key == 13) {                    
                                        var ele=document.getElementById('{!$Component.BillForm.billPageid.billSearchId}');                                                                                       
                                        ele.click();                     
                                        return false;               
                                } else{                    
                                        return true;               
                                }          
                        }     

                function noenterShip(e){
                                if(window.event){                    
                                        key = window.event.keyCode;     //IE               
                                } 
                                else{                    
                                        key = e.which;     //firefox               
                                }               
                                if(key == 13) {                    
                                        var ele=document.getElementById('{!$Component.ShipForm.shipPageid.shipSearchId}');                                                                                       
                                        ele.click();                     
                                        return false;               
                                } else{                    
                                        return true;               
                                }          
                        }     

                                          
        </script>
